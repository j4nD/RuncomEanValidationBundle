{
    "name": "runcom/ean-validation-bundle",
    "description": "Validator for the International Article Number (EAN)",
    "keywords": ["barcode", "ean", "ean13", "ean8"],
    "homepage": "https://github.com/runcom/RuncomEanValidationBundle",
    "type": "symfony-bundle",
    "license": "GPL-3.0+",
    "autoload": {
        "psr-4": { "Runcom\\EanValidationBundle": "" },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },    
    "target-dir": "Runcom/EanValidationBundle",
    "require": {
        "php": ">=5.3.3",
        "symfony/validator": "~2.3|~3.0",
        "symfony/framework-bundle": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "authors": [
        {
            "name": "Antonio Murdaca",
            "email": "me@runcom.ninja"
        }
    ],
    "minimum-stability": "stable"
}
